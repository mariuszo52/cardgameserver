<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.2.xsd">

    <changeSet id="0006" author="mariuszo52">
        <createTable tableName="ability_influence_fraction">
            <column name="ability_id" type="bigint">
                <constraints foreignKeyName="fk_ability_2"
                             referencedTableName="ability"
                             referencedColumnNames="id"/>
            </column>
            <column name="fraction_id" type="bigint">
                <constraints foreignKeyName="fk_fraction_2"
                             referencedTableName="fraction"
                             referencedColumnNames="id"/>
            </column>
        </createTable>
    </changeSet>

</databaseChangeLog>
